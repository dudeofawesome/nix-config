{
  "$schema": "vscode://schemas/settings/user",

  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "material-product-icons",
  "workbench.colorTheme": "Material Theme Lighter",
  "workbench.preferredDarkColorTheme": "Bearded Themanopia (Experimental)",
  "workbench.preferredLightColorTheme": "Material Theme Lighter",
  "window.autoDetectColorScheme": true,
  "workbench.colorCustomizations": {
    "[Material Night]": {
      "editor.findMatchBackground": "#fff17695"
    },
    "[Material Theme Lighter]": {
      // "terminal.foreground": "#CCCCCC",
      // "terminal.background": "#243239",
      "terminal.findMatchBorder": "#ff00FF88",
      "terminal.findMatchBackground": "#ff00FF88",
      "terminal.findMatchHighlightBackground": "#ff00FF88",
      "terminal.findMatchHighlightBorder": "#ff00FF88",
      // "debugConsole.sourceForeground": "#ff0000",
      // "debugConsole.infoForeground": "#ff0000",
      "debugConsole.warningForeground": "#FFB62C",
      "debugConsole.errorForeground": "#F76D47"
    },
    "[Material Theme]": {
      // "terminal.foreground": "#CCCCCC",
      // "terminal.background": "#243239",
      "terminal.findMatchBorder": "#ff00FF88",
      "terminal.findMatchBackground": "#ff00FF88",
      "terminal.findMatchHighlightBackground": "#ff00FF88",
      "terminal.findMatchHighlightBorder": "#ff00FF88",
      // "debugConsole.sourceForeground": "#ff0000",
      // "debugConsole.infoForeground": "#ff0000",
      "debugConsole.warningForeground": "#FFB62C",
      "debugConsole.errorForeground": "#F76D47"
    },
    "terminal.selectionBackground": "#ff00FF88"
  },
  "editor.tokenColorCustomizations": {
    "[Material Theme]": {
      "textMateRules": [
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[Material Theme Lighter]": {
      "textMateRules": [
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "string.quoted.other",
          "settings": {
            "foreground": "#91B859",
            "fontStyle": "bold"
          }
        }
      ]
    },
    "[Material Theme Lighter High Contrast]": {
      "textMateRules": [
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    }
  },

  "workbench.list.horizontalScrolling": true,
  "workbench.editor.enablePreview": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.revealIfOpen": true,

  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.fontLigatures": "'ss01', 'ss04', 'ss06', 'ss07', 'zero'",

  "editor.cursorBlinking": "smooth",

  "editor.tabSize": 2,
  "editor.rulers": [80, 120, 140],
  "editor.autoIndent": "full",

  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.smoothScrolling": true,
  "editor.scrollbar.scrollByPage": false,
  "editor.stickyScroll.enabled": true,
  "editor.mouseWheelScrollSensitivity": 0.4,

  "editor.snippetSuggestions": "inline",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vsintellicode.modelDownloadPath": "/var/lib/vsintellicode/",

  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": true,
  "editor.colorDecorators": true,

  "explorer.openEditors.visible": 0,
  "window.restoreWindows": "folders",
  "window.clickThroughInactive": false,

  "debug.enableAllHovers": true,
  "debug.toolBarLocation": "docked",
  "debug.focusWindowOnBreak": false,
  "debug.console.acceptSuggestionOnEnter": "on",
  "debug.javascript.autoAttachFilter": "smart",

  // "remote.SSH.useLocalServer": false,
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.remotePlatform": {
    "oc.orleans.io": "linux",
    "doa-server.local": "linux",
    "doa-server": "linux",
    "doa-miner": "linux",
    "edgar-plotter": "linux",
    "doa-plotter": "linux",
    "doa-desktop-plotter": "linux",
    "edgar-desktop-plotter": "linux",
    "printer-pi": "linux",
    "home.saldivar.io": "linux",
    "doa-pi-lights": "linux",
    "powell.place": "linux"
  },

  "terminal.external.osxExec": "iTerm.app",
  "terminal.external.windowsExec": "C:\\Users\\DudeOfAwesome\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "terminal.integrated.rightClickBehavior": "default",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.shell.windows": "arch.exe",
  "terminal.integrated.shellArgs.windows": ["run"],
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.smoothScrolling": false,
  "terminal.integrated.macOptionClickForcesSelection": true,
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.minimumContrastRatio": 4.5,
  // "terminal.integrated.mouseWheelScrollSensitivity": 1,
  "accessibility.signals.terminalBell": {
    "sound": "on",
    "announcement": "auto"
  },

  "extensions.autoUpdate": true,

  "files.associations": {
    ".env": "dotenv",
    ".npmignore": "gitignore",
    "*.grule": "go",
    "*.mustache": "handlebars",
    "*.ign": "json",
    ".htmlhintrc": "jsonc",
    "**/sites-available/*.*": "NGINX",
    "**/sites-enabled/*.*": "NGINX",
    "**/snippets/*.*": "NGINX",
    "Appfile": "ruby",
    "Fastfile": "ruby",
    "*.bu": "yaml",
    ".prettierrc": "yaml",
    "*.travis": "yaml",
    "*.graphqlrc": "yaml"
  },
  "files.exclude": {
    "**/.git/": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules/": true,
    "**/build": true,
    "**/dist/": true,
    "**/coverage/": true,
    "**/*.eslintcache": true,
    "**/.devenv/": true
  },

  "testing.openTesting": "neverOpen",

  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },

  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "never",

  "liveshare.featureSet": "insiders",
  "liveshare.allowGuestDebugControl": true,

  "diffEditor.ignoreTrimWhitespace": false,

  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.format.enable": false,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  // "eslint.validate": [
  //   "javascript",
  //   "javascriptreact",
  //   "typescript",
  //   "typescriptreact"
  // ],
  "jest.runMode": {
    "type": "on-demand"
  },
  "jest.outputConfig": {
    "revealOn": "demand",
    "revealWithFocus": "none",
    "clearOnRun": "none"
  },
  "nodejs-testing.extensions": [
    {
      "extensions": ["mjs", "cjs", "js"],
      "parameters": []
    },
    {
      "extensions": ["mts", "cts", "ts"],
      "parameters": ["--loader", "tsm"]
    }
  ],

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.format.enable": false,

  "[ruby]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "ruby.useLanguageServer": true,
  "ruby.useBundler": true,
  "ruby.pathToBundler": "${userHome}/.rbenv/shims/bundler",

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.schemas": [
    {
      "fileMatch": ["tsconfig.*.json"],
      "url": "https://json.schemastore.org/tsconfig.json"
    },
    {
      "fileMatch": [".angular-cli.json"],
      "url": "./node_modules/@angular/cli/lib/config/schema.json"
    },
    {
      "fileMatch": [".stylelintrc*"],
      "url": "https://json.schemastore.org/stylelintrc.json"
    }
  ],

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.suggest.html5": true,
  "htmlhint.documentSelector": ["html", "htm", "mustache"],

  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[handlebars]": {
    "editor.defaultFormatter": "mfeckies.handlebars-formatter"
  },

  "[python]": {
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "python.languageServer": "Pylance",
  "python.jediEnabled": false,
  "python.defaultInterpreterPath": "python3",
  "python.showStartPage": false,
  "jupyter.askForKernelRestart": false,

  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },

  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "bounded",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wrappingIndent": "indent"
  },
  "markdown.extension.orderedList.marker": "one",
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.styles": [
    "https://use.fontawesome.com/releases/v6.1.2/css/all.css"
  ],

  "[nix]": {
    "editor.defaultFormatter": "jnoortheen.nix-ide"
  },
  "nix.enableLanguageServer": true,
  "nix.serverPath": "nil",
  "nix.serverSettings": {
    "nil": {
      "formatting": {
        "command": ["nixpkgs-fmt"]
      }
    }
  },

  "[fish]": {
    "editor.defaultFormatter": "bmalehorn.vscode-fish",
    "editor.tabSize": 4,
    "editor.formatOnPaste": false
  },

  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode",
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|\\(\\*|--)\\s*($TAGS)"
  },
  "Prettier-SQL.SQLFlavourOverride": "postgresql",

  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },

  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "dart.sdkPath": "/Users/dudeofawesome/Library/Developer/flutter/bin/cache/dart-sdk",
  "dart.flutterHotReloadOnSave": "always",
  "dart.debugSdkLibraries": true,
  "dart.debugExternalLibraries": false,
  "dart.showTodos": true,
  "dart.previewFlutterUiGuides": true,
  // "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.openDevTools": "never",
  "dart.devToolsTheme": "dark",
  "dart.debugExternalPackageLibraries": true,

  "stylelint.enable": true,
  "css.validate": false,
  "scss.validate": false,

  "arduino.path": "/Applications",
  "arduino.commandPath": "Contents/MacOS/Arduino",
  "arduino.logLevel": "info",
  "arduino.enableUSBDetection": true,
  "arduino.disableTestingOpen": false,
